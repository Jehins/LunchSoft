@page "/"
@inject NavigationManager NavigationManager
@code{
    string Name = "";

    protected override void OnInitialized()
    {
        if(Sesion.Empleado.Name == "")
        {
            NavigationManager.NavigateTo("/login");
            return;
        }
        else
        {
            Name = Sesion.Empleado.Name;
        }
    }
}
<style>
    .body{
        background-color: #F2F2F2;
        color: #333333;
    }
</style>